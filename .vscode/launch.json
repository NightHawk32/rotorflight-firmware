    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug F405",
            "cwd": "${workspaceFolder}",
            "executable": "${workspaceRoot}/obj/main/rotorflight_STM32F405.elf",
            "request": "launch",
            "interface": "swd",
            "device": "STM32F405VG",
            "type": "cortex-debug",
            "runToEntryPoint": "main",
            "servertype": "jlink",
            "preLaunchTask": "build-F405",
            "serverpath": "C:/Program Files/SEGGER/JLink_V794i/JLinkGDBServerCL.exe",
        },
        {
            "name": "Debug H743",
